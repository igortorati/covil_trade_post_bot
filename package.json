{
  "name": "tradepost",
  "version": "1.0.0",
  "description": "Bot for Trade Post DDAL",
  "main": "index.js",
  "scripts": {
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "dev": "tsx watch src/index.ts",
    "deploy-commands": "tsx src/deployCommands.ts",
    "reset": "rimraf node_modules package-lock.json && npm install",
    "build": "tsc",
    "migrate": "sequelize-cli db:migrate --config sequelize.config.js",
    "migrate:undo": "sequelize-cli db:migrate:undo --config sequelize.config.js",
    "start": "npm run migrate && npm run dev"
  },
  "author": "Igor H. T. Ruy",
  "dependencies": {
    "discord.js": "^14.18.0",
    "discord.js-rate-limiter": "^1.3.2",
    "dotenv": "^16.5.0",
    "mysql2": "^3.14.0",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.2",
    "sequelize-typescript": "^2.1.6",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "rimraf": "^6.0.1",
    "sequelize-typescript-migration": "^0.0.1-beta.3",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3"
  }
}
