{
  "name": "tradepost",
  "version": "1.0.0",
  "description": "Bot for Trade Post DDAL",
  "main": "index.js",
  "scripts": {
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "build-old": "tsc --project tsconfig.json",
    "commands:view": "npm run build && node --enable-source-maps dist/view-commands.js commands view",
    "commands:register": "npm run build && node --enable-source-maps dist/register-commands.js commands register",
    "commands:clear": "npm run build && node --enable-source-maps dist/clear-commands.js commands clear",
    "start-old": "npm run start:bot",
    "dev": "tsx watch src/index.ts",
    "deploy-commands": "tsx src/deployCommands.ts",
    "build": "tsup src/index.ts --minify",
    "start": "ts-node src/index.ts",
    "migrate": "sequelize db:migrate",
    "migration:generate": "sequelize-typescript-migration generate -o src/migrations -c src/config/database.ts"
  },
  "author": "Igor H. T. Ruy",
  "dependencies": {
    "discord.js": "^14.18.0",
    "discord.js-rate-limiter": "^1.3.2",
    "dotenv": "^16.5.0",
    "mysql2": "^3.14.0",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "sequelize-typescript-migration": "^0.0.1-beta.3",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3"
  }
}
